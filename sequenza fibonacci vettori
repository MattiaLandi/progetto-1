// Example program
#include <iostream>
using namespace std;

int main() {
    const int n = 15;  
    int fibonacci[n];

    
    fibonacci[0] = 0;
    fibonacci[1] = 1;

    
    for (int i = 2; i < n; ++i) {
        fibonacci[i] = fibonacci[i - 1] + fibonacci[i - 2];
    }

    
    cout << "I primi 15 elementi della serie di Fibonacci sono"<<n<<endl;
    for (int i = 0; i < n; ++i) {
      cout << fibonacci[i] << " ";
    }

    return 0;
}
