#include<iostream>
#include<math.h>
using namespace std;
int main()
{
double a, b, c, x1, x2;
cout<<"inserisci il valore di a:";
cin>>a;
cout<<"inserisci il valore di b:";
cin>>b;
cout<<"inserisci il valore di c:";
cin>>c;
x1=-b+sqrt(b*b-4*a*c)/(2*a);
x2=-b-sqrt(b*b-4*a*c)/(2*a);
if(b*b-4*a*c<0) {
cout<<"Impossibile"<<endl;
}
else {
cout<<x1<<endl;
cout<<x2<<endl;
}
}
