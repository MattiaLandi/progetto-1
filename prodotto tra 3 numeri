#include <iostream>
using namespace std;

int prodotto(int x,int y, int z){
    int prodotto;
    prodotto= x*y*z;
    return prodotto;
}
int main() {
    int a;
    int b;
    int c;
    int risultato;
    cout<<"inserisci a "<<endl;
    cin>>a;
    cout<<"inserisci b"<<endl;
    cin>>b;
    cout<<"inserisci c"<<endl;
    cin>>c;
    risultato=prodotto(a,b,c);
    cout<<risultato<<endl;
    return 0;
}
     
